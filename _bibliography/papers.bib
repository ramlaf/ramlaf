@article {MR4583772,
    AUTHOR = {B\"{o}hm, Christoph and Lafuente, Ramiro A.},
     TITLE = {Non-compact {E}instein manifolds with symmetry},
   JOURNAL = {J. Amer. Math. Soc.},
  FJOURNAL = {Journal of the American Mathematical Society},
    VOLUME = {36},
      YEAR = {2023},
    NUMBER = {3},
     PAGES = {591--651},
      ISSN = {0894-0347,1088-6834},
   MRCLASS = {53C25 (14L24 53C21 53C30)},
  MRNUMBER = {4583772},
       DOI = {10.1090/jams/1022},
       URL = {https://doi.org/10.1090/jams/1022},
abstract = {For Einstein manifolds with negative scalar curvature admitting an isometric action of a Lie group G with compact, smooth orbit space, we show that the nilradical N of G acts polarly and that the N-orbits can be extended to minimal Einstein submanifolds. As an application, we prove the Alekseevskii conjecture: Any homogeneous Einstein manifold with negative scalar curvature is diffeomorphic to a Euclidean space.},
}
